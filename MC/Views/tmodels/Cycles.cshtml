@{
    ViewBag.Title = "Cycles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Index</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <h4>tmodel</h4>
    <make>
        Make:
        <select style="margin-left:5px">
            <option>Yamaha</option>
            <option>Suzuki</option>
            <option>Honda</option>
        </select>
    </make>
    <br />
    <year>
        Year:
        <select style="margin-left:10px">
            @foreach (SelectListItem @item in ViewBag.year_id)
            {
                <option>@item.Value</option>
            }
        </select>
    </year>
    <br />
    <model>
        Model:
        <select>
            <option>YR1</option>
            <option>YR2</option>
            <option>YR3</option>
            <option>RD350</option>
            <option>RD400</option>
            <option>RZ350</option>
            <option>RZ500</option>
        </select>
    </model>
    <br />
    <div class="form-group-sm">
        <div class="row">
            <div class="col-md-3">
                <h2>Motorcycles</h2>
                <ul>
                    <li>Buying</li>
                    <li>Selling</li>
                    <li>Renting</li>
                </ul>
            </div>
            <div class="col-md-3">
                <h2>Parts</h2>
                <ul>
                    <li><a href="https://go.microsoft.com/fwlink/?LinkID=398600">All</a></li>
